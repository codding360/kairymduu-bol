{
  "name": "kairymduu-bol",
  "description": "Kairymduu Bol is a platform for crowdfunding campaigns in Kyrgyzstan",
  "workspaces": [
    "studio",
    "frontend"
  ],
  "scripts": {
    "dev": "npm-run-all --parallel --print-label dev:*",
    "dev:next": "npm run dev --workspace=frontend",
    "dev:studio": "npm run dev --workspace=studio",
    "format": "prettier --cache --write .",
    "import-sample-data": "cd studio && sanity dataset import sample-data.tar.gz --replace",
    "lint": "npm run lint --workspace=frontend",
    "type-check": "npm run type-check --workspaces"
  },
  "prettier": "@sanity/prettier-config",
  "devDependencies": {
    "@sanity/prettier-config": "^2.0.2",
    "npm-run-all2": "^5.0.2",
    "prettier": "^3.7.3"
  },
  "packageManager": "yarn@4.1.1+sha512.ec40d0639bb307441b945d9467139cbb88d14394baac760b52eca038b330d16542d66fef61574271534ace5a200518dabf3b53a85f1f9e4bfa37141b538a9590"
}
